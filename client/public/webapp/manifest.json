{
    "_version": "1.14.0",
    "sap.app": {
        "_version": "1.14.0",
        "id": "de.hms",
        "type": "application",
        "i18n": "i18n/i18n.properties",
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
        "applicationVersion": {
			"version": "0.0.1"
		}
    },
    "sap.ui":{
        "_version": "1.14.0",
		"technology": "UI5",
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_bluecrystal"
		]
    },
    "sap.ui5": {
        "_version": "1.14.0",
		"rootView": {
			"viewName": "de.hms.view.App",
			"type": "XML",
			"id": "app"
		},
		"handleValidation": true,
        "autoPrefixId": true,
        "dependencies": {
            "minUI5Version": "1.60"
        },
        "resources":{ 
            "css": [{
                "uri": "css/style.css"
            }]
        },
		"models": {			
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "de.hms.i18n.i18n"
				}
			}
		}
    }
}